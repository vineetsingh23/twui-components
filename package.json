
{
  "name": "@vineetsingh23/unique-twreact-components",
  "version": "0.1.0",
  "description": "Example of a reusable UI component library using React, Vite, Tailwind CSS, and TypeScript. Includes Button, Card, and Breadcrumb components.",
  "type": "module",
  "main": "./dist/unique-twreact-components.umd.js",
  "module": "./dist/unique-twreact-components.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/unique-twreact-components.es.js",
      "require": "./dist/unique-twreact-components.umd.js"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit --project tsconfig.json"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.5.1",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4"
  },
  "keywords": [
    "react",
    "typescript",
    "tailwindcss",
    "ui",
    "components",
    "library"
  ],
  "author": "Vineet Kumar Singh",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https:https://github.com/vineetsingh23/twui-components.git"
  },
  "bugs": {
    "url": "https://github.com/vineetsingh23/twui-components/issues"
  },
  "homepage": "https://github.com/vineetsingh23/twui-components.git",
  "publishConfig": {
    "access": "public"
  }
}
